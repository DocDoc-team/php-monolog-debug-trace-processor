{
    "name": "docdoc/php-monolog-debug-trace-processor",
    "description": "Processor adds extra trace to log",
    "homepage": "https://github.com/DocDoc-team/php-monolog-debug-trace-processor",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "monolog",
        "processor",
        "trace"
    ],
    "authors": [
        {
            "name": "apavlov",
            "email": "apavlov@docdoc.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.1"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "DocDoc\\LogTraceProcessor\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    }
}
